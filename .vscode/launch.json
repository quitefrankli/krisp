	"version": "0.2.0",
	"configurations": [
		{
			"name": "krisp",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/build/applications/krisp/krisp",
			"args": [],
			"cwd": "${workspaceFolder}",
			"initCommands": [
				"type category enable default",
				"type category enable libcxx",
				"type category enable gnu-libstdc++"
			],
			"preLaunchTask": "build krisp"
		},
		{
			"name": "space",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/build/applications/space/space",
			"args": [],
			"cwd": "${workspaceFolder}",
			"initCommands": [
				"type category enable default",
				"type category enable libcxx",
				"type category enable gnu-libstdc++"
			],
			"preLaunchTask": "build space"
		}
	]
}
